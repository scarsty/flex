!---------Sigma input and output ----------------------------------------------
sigma_input=.true.,  sigma_input_file="sig_output"             ! input
sigma_output=.true., sigma_output_file="sig_output"             ! output

!----------FEA parameters ------------------------------------------------
sigma_tol=1.0d-4, max_it= 50             ! tolerance in sigma, maximum iterations
alpha=0.3d0,  alpha_scheme=1       ! alpha, (0 constant, 1 using Sig_HF, 2 using Sig_e0)
spin_state=1 ! spin state, 1 or 3
/

&band
!***********-Band structure-****************************************
!--------------orbital level-------------
